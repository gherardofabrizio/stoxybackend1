{
  "name": "stoxy-backend",
  "version": "0.1.0",
  "private": true,
  "description": "Web service for Stoxy mobile application",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc --rootDir . && copyfiles \"./{src,dependencies}/**/*.{txt,mjml,json,css,svg,html,png,mustache,sql,js}\" ./package.json ./package-lock.json ./dist",
    "start": "node -r ts-node/register --preserve-symlinks ./src/index.ts"
  },
  "author": "Sergey Gerasimov",
  "license": "UNLICENSED",
  "dependencies": {
    "@radx/radx-backend-auth": "^0.7.0",
    "@radx/radx-backend-express": "^0.5.7",
    "@radx/radx-backend-knex": "^0.4.1",
    "@radx/radx-backend-swagger-docs": "^1.0.0",
    "copyfiles": "^2.4.1",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "objection": "^2.2.14",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/yargs": "^16.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
}
